import java.util.Scanner;
public class aritmatiksayı {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input= new Scanner(System.in);
		System.out.println("bir sayı gir");
		int n= input.nextInt();
		int s=0;
		int toplam=0;
		for(int i=1;i<n;i++)
		{
			if(n%i==0)
			{
				toplam+=i;
				s++;
			}
		}
		if(toplam % s==0)
		{
			System.out.println("sayı aritmatiktir");
		}
		else
		{
			System.out.println("sayı aritmatik değil");
		}

	}
}
