import java.util.Scanner;
public class girilenpayvepaydayagöresayınınistenilenbasamağınıhesaplamayanprogram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        Scanner input =new Scanner(System.in);
        System.out.println("payı giriniz");
        int pay=input.nextInt();
        System.out.println("paydayı giriniz");
        int payda= input.nextInt();
        System.out.println("istenilen basamak değerini giriniz");
        int d= input.nextInt();
        int B=pay/payda;
	int k=0;
        for(int i=1;i<=d;i++)
        {
            k=B%10;
            B=B/10;
           
        }
	System.out.println("istenilen basamak değeri = "+k);
            
        
     
        
	}

}
