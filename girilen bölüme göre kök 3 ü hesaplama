import java.util.Scanner;
public class girilenbölümsayısınagörekökikisayısınıhesaplama {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input= new Scanner(System.in);
		System.out.println("bölüm sayısını gir");
		int bölüm= input.nextInt();
		int t=1;
		for(int i=1;i<=bölüm-1;i++)
		{
			t=1+1/(2+1/t);
			
		}
		System.out.println(1+1/t);


	}

}
