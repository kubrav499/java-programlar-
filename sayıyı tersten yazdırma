import java.util.Scanner;
public class girilensayıyıterstenyazdırma {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner input=new Scanner(System.in);
		System.out.println("sayıyı gir");
		int n=input.nextInt();
		int c;
		while(n>0)
		{
		     c=n%10;
		     n=n/10;
		     System.out.print(c);
			
		}
		

	}

}
